# Copy this file to .env.local and set your Vida API token.
# These are used when fetching or creating accounts on Vida.

# Server-only Vida API base URL (rarely needs changing)
VIDA_API_TOKEN=
VIDA_RESELLER_ID=
VIDA_API_BASE_URL=https://api.vida.dev/api/v2

# Client-visible config (NEXT_PUBLIC_ will be exposed to the browser)

# App display name in the UI
NEXT_PUBLIC_APP_NAME=Alma
# Vida web app embed base URL
NEXT_PUBLIC_VIDA_EMBED_BASE_URL=https://vida.io/app/embed
# Vida embed button script URL
NEXT_PUBLIC_VIDA_BUTTON_SCRIPT_SRC=https://vida.io/embed/button/v1/scriptMinified.js
# Default Vida target/username for the demo button
NEXT_PUBLIC_VIDA_TARGET=alma
